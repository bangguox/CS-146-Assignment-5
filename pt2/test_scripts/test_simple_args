echo testing arbitrary commands with argument (and options) passed -- final ls will throw error to show file deleted
echo ls of current directory
ls
echo ls of file parse.h
ls parse.h
echo ls with option -l of current directory
ls -l
echo ls with option -l of file: parse.h
ls -l parse.h
echo ls with option -l of file: parse.h and parse.c
ls -l parse.h parse.c
echo cat of file parse.h
cat parse.h
echo creating the file: test_file
touch test_file
echo displaying the newly created file
ls -l test_file
echo removing the file: test_file
rm -f test_file
ls -l test_file
